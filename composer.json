{
    "name": "bavragor/faker-core",
    "type": "library",
    "description": "Faker is a PHP library that generates fake data for you.",
    "keywords": [
        "faker",
        "fixtures",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Zaninotto"
        },
        {
            "name": "Kevin Mauel"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "bavragor/faker-en-US": "^1.0"
    },
    "require-dev": {
        "ext-intl": "*",
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5.8"
    },
    "autoload": {
        "psr-4": {
            "Faker\\": "src/Faker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Test\\": "test/Faker/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
